@charset "utf-8";
/* CSS Document */
* {
	margin: 0px;
	padding: 0px;
}
img {
	border: 0px;
}
ul, li {
	padding:0;
	margin:0;
	list-style:none;
}
.fl {
	float:left
}
.fr {
	float:right;
}
.clear {
	clear:both;
}
.textarea, .textarea_on {
	border:none;
	background: #fff;
	color:#333;
	line-height:18px;
	overflow: visible;
	outline:none;
	padding:2px;
}
.textarea_on {
	background:#fcffd3;
}
.input_text, .input_dj, .input_sl, .input_line {
	height:20px;
	line-height:20px;
	padding:1px 2px;
	border:none;
	background:#ffffff;
	outline:none;
}
.input_texton, .input_djon, .input_slon, .input_lineon {
	height:20px;
	line-height:20px;
	padding:1px 2px;
	border:none;
	background:#fcffd3;
	outline:none;
}
.input_texton{
	background:#fcffd3 url(../images/error.png) no-repeat right center;
}
.input_texton_line{
	/*padding:3px 0px;
	border:none;
	border-bottom:1px solid #ddd;*/
	background:#fcffd3 url(../../../Scripts/themes/default/images/error.png) no-repeat right center;
	outline:none;
}
.input_line, .input_lineon {
	border-bottom:1px solid #ddd;
}
.input_dj, .input_djon {
	text-align:right;
}
.input_sl, .input_slon {
	text-align:center;
}
.input_zcfl, .input_zcflon {
	height:20px;
	line-height:20px;
	padding:2px 20px 2px 2px;
	border:none;
	outline:none;
	/*background:#ffffff;*/
}
.input_zcflon {
	background:#fcffd3;
}
.input_sanjiao{
	border:none;
	height:20px;
	width:20px;
	cursor:pointer;
	background:url(../images/input_sanjiao.gif) no-repeat 5px 5px;
}
.form_div {
	/*width:750px;*/
	margin:5px 15px 15px 15px;
	border:1px solid #d2d2d2;
	/*padding:12px;    /*2016-6-14 chongs*/*/
	padding-top: 10px;  /*2017-8-11 chongs*/
	box-shadow:3px 3px 0 #efefef;
	filter:progid:DXImageTransform.Microsoft.Shado(Strength=3, Direction=120, Color='#efefef');
	-webkit-box-shadow:3px 3px 0 #efefef;
	-moz-box-shadow:3px 3px 0 #efefef;
	background:#ffffff;
}
.form_div h2 {
	padding-top:20px;
	font-size:28px;
	font-weight:bold;
	text-align:center;
}
.form_top label span{
margin:0 3px 0 0;
color:#F00;
	/**margin-left:33px;*/
}
.title_left {
	float:left;
	line-height:24px;
	padding:8px;
	width:440px;
}
.title_left p {
	height:36px;
}
.title_left p label {
	float:left;
	width:70px;
	text-align:right;
	line-height:24px;
	margin-right:5px;
}

.input_label {
	float:left;
	width:70px;
	text-align:right;
	line-height:24px;
	margin-right:5px;
}
.title_right {
	float:right;
	padding:8px;
	width:260px;
	text-align:right;
}
.title_right .code {
	/*background: url(../images/code.gif) no-repeat top right;*/
	height:30px;
}
.title_right .nom {
	line-height:16px;
}
.title_right .nom span {
	/*letter-spacing:2px;*/
}
.list_sytle {
	border-right:2px solid #95b8e7;
	clear:both;
}
.list_sytle th {
	border-bottom:1px solid #ddd;
	border-right:1px solid #ddd;
	border-top:2px solid #95b8e7;
	background:#f5f5f5;
	height:26px;
	line-height:26px;
	text-align:center;
	font-weight:normal;
}
.list_sytle td {
	border-bottom:1px solid #ddd;
	border-right:1px solid #ddd;
	height:26px;
	line-height:26px;
	padding:0px 0 0 4px;
}
.minus, .plus {
	width:12px;
	height:12px;
	display:block;
	border:none;
	background:#fff;
	cursor:pointer;
	background: url(../images/minus.gif) no-repeat center center;
}
.plus {
	background: url(../images/plus.gif) no-repeat center center;
}
.form_foot {
/*margin-left:25px;
	*margin-left:33px;
	border-top:1px solid #95b8e7;*/
}
.form_footleft {
	padding:8px;
	float:left;
	line-height:18px;
	white-space: nowrap;
	word-break: keep-all;
}
.form_footleft p label {
	float:left;
	line-height:24px;
	margin-right:5px;
	text-align:right;
}
.form_footright {
	padding:8px;
	float:right;
	line-height:18px;
	white-space: nowrap;
	word-break: keep-all;
}
.form_footright p label {
	float:left;
	line-height:24px;
	margin-right:5px;
	text-align:right;
}
.form_footright span {
	font-size:14px;
	font-weight:bold;
}
.form_footright .no-span {
	font-size:12px;
	font-weight: normal;
}
.form_footcenter {
	width:140px;
	margin:0 auto;
	padding:8px;
}
.zcfl {
	border:1px solid #9c9c9c;
	background:#fff;
	margin-top:0px;
	margin-left:0px;
	padding:3px;
	position:absolute;
	z-index:10;
	cursor:pointer;
}
.zcfl p {
	font-size:14px;
	line-height:30px;
	padding:0 5px;
	white-space: nowrap;
	word-break: keep-all;
}
.zcfl p:hover {
	background:#c2eaff;
}
.zcfl p span {
	padding:0 5px;
}
.form_table { 
	
	/*border-bottom:1px solid #95b8e7;*/
	/*width:750px;*/
	/*border:1px solid #ddd;*/
	border-collapse:collapse;/*margin-left:25px;
	*margin-left:33px;*/
	/*background:#f8f8f8;*/
	color:#515151;
	
} /*2016-6-14 chongs*/
.form_table th {
	/*border-top:1px solid #ddd;
	border-bottom:1px solid #ddd;*/
	/*background:#f5f5f5;*/
	border-collapse:collapse;
	text-align:left;
	font-weight:bold;
	line-height:26px;
	padding:5px 0;
	padding-left:30px;
	font-weight:normal;
	
}/*2016-6-14 chongs*/
.form_table td {
	/*border-top:1px solid #ddd;
	border-bottom:1px solid #ddd;*/
	border-collapse:collapse;
	line-height:22px;
	padding:5px;
}/*2016-6-14 chongs*/

/*2016-6-15 chongs*/
.form_table_show { 
	
	border-bottom:1px solid #95b8e7;
	/*width:750px;*/
	border:1px solid #ddd;
	border-collapse:collapse;/*margin-left:25px;
	*margin-left:33px;*/
	background:#fff;
	color:#515151;
} 
.form_table_show th {
	border-top:1px solid #ddd;
	border-bottom:1px solid #ddd;
	background:#f5f5f5;
	border-collapse:collapse;
	text-align:left;
	font-weight:bold;
	line-height:26px;
	padding:5px 0;
	padding-left:30px;
	font-weight:normal;
}
.form_table_show td {
	border-top:1px solid #ddd;
	border-bottom:1px solid #ddd;
	border-collapse:collapse;
	line-height:22px;
	padding:5px;
}



.form_black {
	border:2px solid #000;
	border-collapse:collapse;
	text-align:center;
}
.form_black u{
	padding:0px 3px;
}
.form_black h3{
	font-size:15px;
	text-align:center;
}
.form_black th {
	border:1px solid #000;
	border-collapse:collapse;
	font-size:15px;
	font-weight:900;
	line-height:18px;
	padding:10px 0;
	font-family:"Arial Black", Gadget, sans-serif;
}
.form_black .weight_th{
	border:2px solid #000;
}
.form_black .weight_th span{
	float:left;
	margin-left:5px;
}
.form_black td {
	border:1px solid #000;
	border-collapse:collapse;
	line-height:18px;
	padding:10px 5px;
	font-size:15px;
}
.form_black .left_td {
	text-align:left;
}
.form_btn {
	margin:0 25px;
/* *margin-left:33px;
	width:790px;*/
}
.form_btn .float_left {
	padding:5px 5px;
	float:left;
}
.form_btn .float_right {
	padding:5px 5px;
	float:right;
}
