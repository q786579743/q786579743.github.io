.field {
	padding:5px 0;
	border:none;
}
.field .legend {
	padding:0 20px 5px 20px;
	margin-left:5px;
	font-weight:bold;
	cursor:pointer;
	background:url(../../../Scripts/themes/default/images/field_arrows.png) no-repeat left -35px;
}
.field .legend_hover {
	padding:0 20px 5px 20px;
	margin-left:5px;
	font-weight:bold;
	cursor:pointer;
	background:url(../../../Scripts/themes/default/images/field_arrows.png) no-repeat left top;
}
.field .legend_hover_detail {
	padding:0 20px 5px 20px;
	margin-left:0px;
	font-weight:bold;
	cursor:pointer;
/*--	background:url(../../../Scripts/themes/default/images/field_arrows.png) no-repeat left top;--*/
}
/*------------------------------------------------*/
.icon_legend {
	padding:5px 20px;
	margin:8px 0 0 5px;
	font-weight:bold;
	font-size:12px;
	background:url(../Scripts/themes/default/images/icon_arrows.png) no-repeat left -14px;
}
/*------------------------------------------------*/
.time_bg01 {
	background:url(../images/time_bg.png) no-repeat top right;
	margin-top:8px;
}
.time_bg02 {
	background:url(../images/time_bg.png) no-repeat 0 -30px;
	height:30px;
}
.form_tabletj {
	border:1px solid #ddd;
	border-collapse:collapse;
}
.form_tabletj th {
	border:1px solid #ddd;
	border-collapse:collapse;
	text-align:right;
	font-weight:bold;
	line-height:49px;
	font-weight: normal;
	font-size:15px;
	padding-right:5px;
}
.form_tabletj td {
	border:1px solid #ddd;
	border-collapse:collapse;
	line-height:45px;
	padding:2px 5px;
	font-weight:bold;
	font-size:15px;
}
.form_tabletj td label {
	float:left;
	width:130px;
	text-align:right;
}
.form_tabletj td span {
	float:right;
}
/*------------------------------------------------*/
.time_bg02 ul li {
	float:left;
	width:60px;
	margin:0px 3px;
	cursor:pointer;
	height:30px;
	line-height:30px;
	text-align:center;
}
.time_bg02 ul .hover, .time_bg02 ul li:hover {
	color:#fff;
	font-weight:bold;
	background:url(../images/time_btn.png) 0 -120px;
}
/*------------------------------------------------*/
.time_bg02 ul .lileft {
	float:left;
	width:30px;
	margin:0 3px 0 0;
	background:url(../images/time_btn.png) 0 0;
}
.time_bg02 ul .lileft:hover {
	background:url(../images/time_btn.png) 0 -30px;
}
/*------------------------------------------------*/
.time_bg02 ul .liright {
	float:right;
	width:30px;
	margin:0 0 0 3px;
	background:url(../images/time_btn.png) 0 -60px;
}
.time_bg02 ul .liright:hover {
	background:url(../images/time_btn.png) 0 -90px;
}
