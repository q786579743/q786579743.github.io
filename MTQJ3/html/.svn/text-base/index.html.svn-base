					text: ""
				});
			}
			//导出蒙版隐藏
			function ExprotMarkHide() {
				$.messager.progress("close");
			}

			function btnBack() {
				$("#Lnk_Cancel").click();
			}
			var dlg;

			function LogOut() {
				//sendTips('身份认证Key已卸载，系统退出登录！', 1, al());
				if(dlg == null) {
					dlg = $.dialog({
						id: "dialogBack",
						content: '身份认证Key已卸载，系统退出登录！',
						max: false,
						min: false,
						title: '消息',
						icon: 'error.gif',
						lock: true,
						ok: function() {
							btnBack();
						},
						close: function() {
							btnBack();
						}
					});
				}
			}

			function MessageProgress(message) {
				$.messager.progress({
					title: "消息提示",
					msg: message,
					text: ""
				});
			}

			function MessageProgressClose() {
				$.messager.progress("close");
			}
