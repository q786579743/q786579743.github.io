	color: #424f63;
	font-size: 30px;
	font-weight: normal;
	font-family: “Microsoft YaHei” ! important;
}

.topBanner {
	float: right;
	height: 80px;
	line-height: 80px;
	*float: none;
}

.topBanner:hover {
	/*background:#F66;*/
	cursor: pointer;
}

.topBanner .user-info {
	float: right;
	width: 60px;
	height: 80px;
	cursor: pointer;
	background: url(../images/user_arr.png) no-repeat 5px center;
}

.topBanner .user-info2 {
	float: right;
	width: 60px;
	height: 80px;
	cursor: pointer;
	/*background: url(../images/user_arr.png) no-repeat 5px center;*/
}
.topBanner .user_icon {
	float: right;
	width: 22px;
	height: 80px;
	padding-left: 35px;
	background: url(../images/icon_user2.png) no-repeat right center;
}

.topBanner .user_icon2 {
	float: right;
	width: 22px;
	height: 80px;
	padding-left: 35px;
	background: url(../images/icon_message.png) no-repeat right center;
}

.topBanner .icon-news,
.topBanner .icon-exit,
.topBanner .icon-help {
	width: 40px;
	float: right;
	padding-left: 40px;
	line-height: 40px;
	margin-top: 20px;
	border-radius: 5px;
	color: #000;
}

.topBanner .icon-exit:hover {
	/*background: #f66;*/
}
.topBanner .icon-news {
	background: url(../images/gly_icon01.png) no-repeat 13px center;
}
.topBanner .icon-exit {
	background: url(../images/icon_exit2.png) no-repeat 13px center;
}

.topBanner .icon-help {
	background: url(../images/icon-help.png) no-repeat 13px center;
}

.top_bg .line {
	height: 100px;
	width: 1px;
	background: #2486e9;
	float: right;
}
/*弹出框*/
#userInfo {
	background: #fff;
	border: 1px solid #1e88cf;
	box-shadow: 0 1px 4px rgba(0, 0, 0, .15);
	display: none;
	overflow: hidden;
	position: absolute;
	right: 5px;
	_right: 25px;
	top: 80px;
	width: 108px;
	z-index: 1;
}

/*top功能菜单弹出层*/

#userInfo ul {
	padding: 5px 0
}

#userInfo li {
	height: 26px;
	display: block;
}

#userInfo a {
	color: #83aed4;
	height: 100%;
	width: 100%
}

#userInfo .group {
	border-bottom: 1px solid #f1f1f1;
}

#userInfo .txt {
	display: block;
	line-height: 26px
}

#userInfo a:hover {
	background-color: #d6f0ff;
	color: #3998e7
}

#userInfo .password .icon,
#userInfo .editinfo .icon,
#userInfo .feedback .icon,
#userInfo .help .icon,
#userInfo .exit .icon {
	float: left;
	height: 26px;
	margin-left: 5px;
	width: 33px;
	background: url(../images/icon_userinfo.png) no-repeat 5px 5px;
}

#userInfo .editinfo .icon {
	background-position: 5px -31px;
}

#userInfo .feedback .icon {
	background-position: 5px -67px;
}

#userInfo .help .icon {
	background-position: 5px -103px;
}

#userInfo .exit .icon {
	background-position: 5px -138px;
}

#userInfo a:hover .icon {
	background: #d6f0ff url(../images/icon_userinfo.png) no-repeat 5px 5px;
}

#userInfo .editinfo a:hover .icon {
	background: #d6f0ff url(../images/icon_userinfo.png) no-repeat 5px -31px;
}

#userInfo .feedback a:hover .icon {
	background: #d6f0ff url(../images/icon_userinfo.png) no-repeat 5px -67px;
}

#userInfo .help a:hover .icon {
	background: #d6f0ff url(../images/icon_userinfo.png) no-repeat 5px -103px;
}

#userInfo .exit a:hover .icon {
	background: #d6f0ff url(../images/icon_userinfo.png) no-repeat 5px -138px;
}

#userInfo a:hover .txt {
	background: #d6f0ff;
	color: #3998e7
}


