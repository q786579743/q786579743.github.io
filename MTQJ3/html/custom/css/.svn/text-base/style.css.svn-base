/*左侧折叠菜单*/

#folders {
	position: relative;
	z-index: 2;
}

.clearfix {
	height: 600px;
	overflow: auto;
	overflow-x: hidden;
}

.child {
	display: block;
	border-top: none;
	display: none;
	line-height: 23px;
	width: 195px;
}

.parent {
	height: 44px;
	line-height: 44px;
	font-size: 14px;
	cursor: pointer;
	color: #475370;
	border-bottom: 1px solid #e5ebf1;
}

.parent.se {
	background: url(../images/side-hoverbg.png) #ddecf5 repeat-x;
}

.parent a {
	display: block;
	padding-left: 25px;
	color: #475370;
}

.parent a img {
	float: left;
	margin: 10px 5px 0 0;
}

.parent a.first {
	/*color:#3083eb;*/
	background: url(../images/side-hoverbg.png) #ddecf5 repeat-x;
}

.child ul {
	display: block;
	margin: 0 15px;
}

.child ul li {
	height: 44px;
	line-height: 44px;
	display: block;
	color: #3B475F;
	margin: 0;
	padding-left: 10px;
	border-bottom: 1px solid #e5ebf1;
	padding-left: 10px;
	background: url(../images/sub_icon.png) 25px center no-repeat;
}

.child ul li:hover {
	color: #3083eb;
	margin: 0;
	padding-left: 10px;
	background: url(../images/sub_icon.png) 25px center no-repeat;
}


/*子目录点击后效果*/

.child ul li.different {
	display: block;
	margin: 0;
	padding-left: 10px;
	background: url(../images/sub_icon_on.png) 25px center no-repeat;
}

.child ul li.different a {
	color: #3587c3;
	font-weight: bold;
}

.child ul li.usual a {
	display: block;
	/**/
}

.child ul li a {
	display: block;
	color: #3B475F;
	padding-left: 35px;
}


