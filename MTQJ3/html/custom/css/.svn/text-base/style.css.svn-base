/*按钮*/

#toolbar {
	margin: 10px 20px 0px 20px;
	height: 46px;
	overflow: hidden;
	padding-left: 0px;
	/*_border-bottom: 1px solid #e5e5e5;*/
}

#toolbar li {
	float: left;
	margin: 0 5px;
	cursor: pointer;
}

.btn_left,
.btn_right,
.btn_back,
.separate1,
.btn_left_on,
.btn_right_on {
	height: 31px;
	line-height: 31px;
	color: #4d7798;
}

.btn_left {
	background: url("../images/btn_left.png") no-repeat left;
	padding-left: 5px;
}

.btn_right {
	background: url("../images/btn_right.png") no-repeat right;
	padding-left: 7px;
	padding-right: 12px;
	display: block;
}

.btn_left:hover {
	background: url("../images/btn_left_over.png") no-repeat left;
	color: #fff;
}

.btn_right:hover {
	background: url("../images/btn_right_over.png") no-repeat right;
	color: #fff;
}

.btn_back {
	background: url("../images/btn_back.png") no-repeat;
	width: 48px;
	text-align: center;
}

.btn_back a {
	color: #4d7798;
}

.btn_back:hover {
	background: url("../images/btn_back_over.png") no-repeat;
}

.btn_back:hover a {
	color: #fff;
}

.btn_left_on {
	background: url("../images/btn_left_on.png") no-repeat left;
	color: #fff;
	padding-left: 5px;
}

.btn_right_on {
	background: url("../images/btn_right_on.png") no-repeat right;
	color: #fff;
	padding-left: 7px;
	padding-right: 12px;
	display: block;
}

.separate1 {
	background: url("../images/line.png") no-repeat center center;
	width: 1px;
}


/*列表*/

.flexthead {
	border-collapse: collapse;
	margin-right: 16px;
}

.flexthead th {
	background: #f5f5f5;
	border-right: 1px solid #cbcbcb;
	border-left: 1px solid #cbcbcb;
	height: 30px;
	line-height: 30px;
	padding-left: 3px;
	color: #4d7798;
	font-weight: normal;
}

.flexthead th.desc span {
	background: url('../images/icon-down.gif') no-repeat right center;
	display: inline-block;
	padding: 0 15px;
}

.flexthead th.asc span {
	background: url('../images/icon-up.gif') no-repeat right center;
	display: inline-block;
	padding: 0 15px;
}

.flexthead_div {
	overflow: auto;
	background: #f5f5f5;
	border-top: 1px solid #cbcbcb;
	border-bottom: 1px solid #cbcbcb;
	border-right: 1px solid #cbcbcb;
}

.flex_div {
	height: 200px;
	overflow: auto;
	padding-bottom: 3px;
	border-left: 1px solid #cbcbcb;
	border-right: 1px solid #cbcbcb;
	border-bottom: 1px solid #cbcbcb;
}

.flex {
	border-collapse: collapse;
}

.flex td {
	border-bottom: 1px solid #ccc;
	border-right: 1px solid #cbcbcb;
	height: 30px;
	padding-left: 3px;
	overflow: hidden;
