}
.idx_funbtn_bl, .idx_funbtn_bc, .idx_funbtn_br {
	height:220px;
}
.idx_funbtn_bl {
	background:url(../images/idx_funbtn_bl.png) no-repeat;
	width:8px;
}
.idx_funbtn_bc {
	background:url(../images/idx_funbtn_bc.png) repeat-x top right;
}
.idx_funbtn_br {
	background:url(../images/idx_funbtn_br.png) no-repeat;
	width:8px;
}

.idx_lingd_bl, .idx_lingd_bc, .idx_lingd_br {
	height:120px;
}
.idx_lingd_bl {
	background:url(../images/idx_lingd_bl.png) no-repeat;
	width:8px;
}
.idx_lingd_bc {
	background:url(../images/idx_lingd_bc.png) repeat-x top right;
}
.idx_lingd_br {
	background:url(../images/idx_lingd_br.png) no-repeat;
	width:8px;
}
.idx_funbtn_bg {
	width:760px;
	margin:0 auto 0 20px;
}
.idx_funbtn_title {
	background:url(../images/idx_funbtn_title.png) no-repeat;
	width:136px;
	height:26px;
	font-size:14px;
	font-weight:bold;
	color:#0276ba;
	text-align:center;
	line-height:28px;
	margin-left:615px;
	*margin-left:595px;
	margin-top:-26px;
	position:absolute;
	z-index:11;
}
.idx_funbtn_bc li,.idx_lingd_bc li {
	padding:0px 12px 8px 12px;
	margin:0 20px;
	float:left;
	width:80px;
	text-align:center;
	cursor:pointer;
	border:1px solid #fff;
	border-radius:5px;
}
.idx_funbtn_bc li a,.idx_lingd_bc li a {
	margin-top:75px;
	padding-top:3px;
	display:block;
}
