@charset "utf-8";
/* CSS Document */
/*系统首页*/

ul, li {
	list-style:none;
	padding:0;
	margin:0;
}

#quick_search {
	padding: 2px 6px 0px 6px;
	top: 80px;
	left: 510px;
	width: 260px;
	height: 22px;
	line-height: 18px;
	position: absolute;
	display:none;
}
#quick_search input.search-text,#quick_search input.search-text-on{
	margin: 0px 3px;
	padding: 0px 2px;
	border:none;
	width: 200px;
	height: 22px;
	line-height: 22px;
	font-size: 12px;
	border-radius: 2px;
	border: 1px solid rgb(187, 187, 187);
	box-shadow: inset 0px 1px 1px #e6e6e6;
	background-color: rgb(255, 255, 255);
	color:#b3b3b3
}
#quick_search input.search-text-on {
	color:#000
}

.fontred {color:red;}
.fontgreen {color:#27800f;}
.fontblue {color:#1768bb;}
.numk1{ width:45px; display:inline-block; text-align:right; margin-right:5px;}
.numk2{ width:65px; display:inline-block; margin-right:5px; }
.numk3{ width:12px; display:inline-block; text-align:center; margin-right:5px; }
.numk4{ width:120px; display:inline-block; text-align:right; margin-left:5px; margin-right:5px;}
.numk5{ width:70px; display:inline-block; text-align:right; margin-right:5px; }
.idx_bg {
	background:#e9f0f7;
	/*background:#ededed;三品一标*/
	border-top:1px #fff solid;
	border-right:1px #bdd1e3 solid;
	border-bottom:1px #bdd1e3 solid;
	padding-bottom:10px;
}
.idx_user {
	height:61px;
