/*
 * 删除
 */
function del() {
	$.dialog.confirm('删除后不可恢复，您确定要删除？', function() {
		$.ajax({
			type: "POST",
			url: "url?o=del&uniqueid=" + $("#Hid_UniqueID").val(),
			success: function(msg) {

				if(msg.toLocaleString().toLowerCase() == "true") {

					sendTips("删除成功", 0, setTimeout(function() {
						$('#pagelist').datagrid('reload');
					}, 1000));
				} else {
					sendTips("对不起，删除失败", 1);
				}
			}
		});
	});

}
